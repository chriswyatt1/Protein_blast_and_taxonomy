nextflow.enable.dsl = 2

manifest {
  description = 'Creating a blast and taxonomy script for proteins' 
  author = 'Chris Wyatt'
}

process {
  
  shell = ['/bin/bash','-e']
  withLabel: "blast" {
    cpus = 4
    container = 'buchfink/diamond'
  }
   withLabel: "blastdb" {
    cpus = 4
    container = 'buchfink/diamond'
  }
}

docker.enabled = true